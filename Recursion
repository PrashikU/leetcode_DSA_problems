//2.Print numbers in decreasing order
public class Dec{
    public static void Dec(int n){
        if(n == 1){
            System.out.println(n);
            return;
        }
       System.out.println(n+" ");
        Dec(n-1);
    }
    public static void main(String[] args) {
        int n = 10;
        Dec(n);  
    }
}
